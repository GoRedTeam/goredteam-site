{
    "plugins": ["prettier-plugin-go-template"],
    "overrides": [
      {
        "files": ["*.html"],
        "options": {
          "parser": "go-template",
          "goTemplateBracketSpacing": true,
          "bracketSameLine": true
        }
      }
    ],
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  }

